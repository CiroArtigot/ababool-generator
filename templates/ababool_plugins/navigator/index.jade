#navigator(ng-show="navigator")
  div(class="navigator" ng-show="navigator")
    div(class="navigator_arrow arrow_right" )
      span(class="glyphicon glyphicon-chevron-right" ng-show="navright" ng-click="gotopage(right,1)")
    div(class="navigator_arrow arrow_left" )
      span(class="glyphicon glyphicon-chevron-left" ng-show="navleft" ng-click="gotopage(left,2)")
    div(class="navigator_arrow arrow_up" )
      span(class="glyphicon glyphicon-chevron-up" ng-show="navup" ng-click="gotopage(up,3)")
    div(class="navigator_arrow arrow_down" )
      span(class="glyphicon glyphicon-chevron-down" ng-show="navdown" ng-click="gotopage(down,4)")
